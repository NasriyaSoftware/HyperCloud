{
  "name": "@ahmadnasriya/hypercloud",
  "version": "0.0.1",
  "description": "Nasriya HyperCloud is a lightwight Node.js HTTP2 framework.",
  "main": "./dist/cjs/hypercloud.cjs",
  "module": "./dist/esm/hypercloud.mjs",
  "types": "./dist/@types/hypercloud.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/hypercloud.cjs",
      "require": "./dist/cjs/hypercloud.mjs",
      "types": "./dist/@types/hypercloud.d.ts"
    }
  },
  "publishConfig": {
    "provenance": true
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/nasriyasoftware/HyperCloud"
  },
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run rename && npm run copyFiles",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "rename": "mod-js",
    "copyFiles": "ns-copySrc",
    "test": "jest",
    "deepTest": "jest --coverage"
  },
  "keywords": [],
  "author": "Nasriya Software, LLC.",
  "license": "Nasriya License",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.12.8",
    "nasriya-copyfiles": "github:nasriyasoftware/CopyFiles",
    "nasriya-modularizejs": "github:nasriyasoftware/ModularizeJS",
    "nodemon": "^3.0.3",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "ms": "^2.1.3",
    "nasriya-cron": "github:nasriyasoftware/nasriyacron",
    "nasriya-dns": "github:nasriyasoftware/nasriyadns",
    "nasriya-logify": "github:nasriyasoftware/logify",
    "openssl-self-signed-certificate": "^1.1.6",
    "redis": "^4.6.14",
    "tldts": "^6.1.11"
  },
  "bugs": {
    "url": "https://github.com/nasriyasoftware/HyperCloud/issues"
  },
  "homepage": "https://github.com/nasriyasoftware/HyperCloud"
}